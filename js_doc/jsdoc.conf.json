{
  "tags": {
    "allowUnknownTags": true
  },
  "plugins": ["plugins/markdown"],
  "opts": {
    "template": "js_doc/template/docdash",
    "encoding": "utf8",
    "recurse": true,
    "verbose": true
  },
  "templates": {
    "default": {
      "outputSourceFiles": false
    },
    "cleverLinks": false,
    "monospaceLinks": false,
    "dateFormat": "ddd MMM Do YYYY"
  },
  "markdown": {
    "parser": "gfm",
    "hardwrap": true
  }
}
